--message--

function init_msg()
	messages={}
end

function create_msg(name,...)
	msg_title=name
	messages={...}
end

function update_msg()
	if (btnp(❎)) then
		deli(messages,1)
	end
end

function draw_msg()
	if messages[1] then
		local y=100
		if p.y%16>9 then
			y=20
		end 
		rectfill(1,y+8,126,y+22,10)
		rectfill(2,y+9,125,y+21,4)
		print(messages[1],4,y+10,7)
		rectfill(1,y+1,
		4+#msg_title*4,y+8,10)
		rectfill(2,y+2,
		3+#msg_title*4,y+8,4)
		print(msg_title,4,y+3,7)
	end	
end
