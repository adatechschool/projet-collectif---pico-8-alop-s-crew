--stars

function create_stars()
	stars={}
	for i=1,14 do
		new_star={
			x=rnd(128),
			y=rnd(128),
			col=1,
			speed=0.5+rnd(0.5)
		}
		add(stars,new_star)
	end

	for i=1,9 do
		new_star={
			x=rnd(128),
			y=rnd(128),
			col=rnd({10,14,15}),
			speed=2+rnd(2)
		}
		add(stars,new_star)
		end
end

function update_stars()
	for s in all(stars) do
		s.y+=s.speed
		if s.y > 128 then
			s.y=0
			s.x=rnd(128)
		end
	end
end
