--ui--

function draw_ui()
	camera(0,0)
	palt(0,false)
	palt(12,true)
	spr(8,2,2)
	print_outline("X"..p.keys1,11,2,7)
	spr(15,20,2)
	print_outline("X"..p.keys2,29,2,7)
	spr(35,38,2)
	print_outline("X"..p.keys3,47,2,7)
	palt()
end

function print_outline(text,x,y)
	print(text,x-1,y,0)
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)	
end
