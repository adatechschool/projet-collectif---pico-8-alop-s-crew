--the alop's adventure--
--by alop's team--

function _init()
	create_player()
	init_msg()
	init_camera()
	create_msg("alop:",
	"me voila sur place\nbon ou est grogu?")
end

function _update()
	if not messages[1] then
		player_movement()
	end
	update_camera()
	update_msg()
end

function _draw()
	cls()
	draw_map()
	draw_player()
	draw_ui()
	draw_msg()
end
