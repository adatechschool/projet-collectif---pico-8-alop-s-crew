function grogu(x,y)
	if x==20 and y==5 then
		create_msg("grogu",
		"sauve moi !\nj'ai peur au secours")		
	end
end

function greedo(x,y)
	if x==23 and y==13 then
		create_msg("greedo",
		"ahah grogu est la\na moi la prime")
	end
end

